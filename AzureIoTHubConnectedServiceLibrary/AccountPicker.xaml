<UserControl x:Uid="UserControl_1"
             x:ClassModifier="internal"
             x:Class="AzureIoTHubConnectedService.AccountPicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid x:Uid="Grid_1"
          Margin="0,2,0,0" >
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="Auto" />
        </Grid.RowDefinitions>

        <ContentControl x:Uid="AccountPickerHost"
                        Grid.Row="0"
                        Name="AccountPickerHost"
                        Loaded="AccountPickerHost_Loaded"
                        Focusable="False"/>
    </Grid>
</UserControl>

